{
  "services": [{
    "type": "rabbitmq",
    "queues": [
      {
        "name": "non-random-queue",
        "message": {
          "frequency": 0.5,
          "body": "Non random message",
          "includeTimestamp": true,
          "includeRandom": false
        }
      },
      {
        "name": "random-queue",
        "message": {
          "frequency": 0.8,
          "body": "Random message",
          "includeTimestamp": true,
          "includeRandom": true,
          "randomConfig": {
            "erlangOrder": 20
          }
        }
      }
    ]
  }]
}